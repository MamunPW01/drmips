<resources>

	<!--
	    Base application theme, dependent on API level. This theme is replaced
	    by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
	-->
	<style parent="android:Theme.Holo" name="DarkTheme">
		<item name="fileIcon">@drawable/ic_action_file_light</item>
		<item name="newIcon">@drawable/ic_action_new_light</item>
		<item name="openIcon">@drawable/ic_action_open_light</item>
		<item name="saveIcon">@drawable/ic_action_save_light</item>
		<item name="saveAsIcon">@drawable/ic_action_save_as_light</item>
		<item name="deleteIcon">@drawable/ic_action_delete_light</item>
		<item name="assembleIcon">@drawable/ic_action_assemble_light</item>
		<item name="stepIcon">@drawable/ic_action_step_light</item>
		<item name="backstepIcon">@drawable/ic_action_backstep_light</item>
		<item name="loadIcon">@drawable/ic_action_load_light</item>
		<item name="aboutIcon">@drawable/ic_action_about_light</item>
		<item name="prefsIcon">@drawable/ic_action_prefs_light</item>
		<item name="themeIcon">@drawable/ic_action_theme_light</item>
		<item name="restartIcon">@drawable/ic_action_restart_light</item>
		<item name="runIcon">@drawable/ic_action_run_light</item>
		<item name="helpIcon">@drawable/ic_action_help_light</item>

		<item name="aux_comp_back">@drawable/aux_comp_back_white</item>

		<item name="wireColor">@color/white</item>
		<item name="readColor">@color/readDark</item>
		<item name="writeColor">@color/writeDark</item>
		<item name="rwColor">@color/rwDark</item>
		<item name="instColor">@color/instDark</item>
		<item name="idColor">@color/idDark</item>
		<item name="ifColor">@color/ifDark</item>
		<item name="exColor">@color/exDark</item>
		<item name="memColor">@color/memDark</item>
		<item name="wbColor">@color/wbDark</item>
	</style>

	<style name="LightTheme" parent="android:Theme.Holo.Light">
		<item name="fileIcon">@drawable/ic_action_file_dark</item>
		<item name="newIcon">@drawable/ic_action_new_dark</item>
		<item name="openIcon">@drawable/ic_action_open_dark</item>
		<item name="saveIcon">@drawable/ic_action_save_dark</item>
		<item name="saveAsIcon">@drawable/ic_action_save_as_dark</item>
		<item name="deleteIcon">@drawable/ic_action_delete_dark</item>
		<item name="assembleIcon">@drawable/ic_action_assemble_dark</item>
		<item name="stepIcon">@drawable/ic_action_step_dark</item>
		<item name="backstepIcon">@drawable/ic_action_backstep_dark</item>
		<item name="loadIcon">@drawable/ic_action_load_dark</item>
		<item name="aboutIcon">@drawable/ic_action_about_dark</item>
		<item name="prefsIcon">@drawable/ic_action_prefs_dark</item>
		<item name="themeIcon">@drawable/ic_action_theme_dark</item>
		<item name="restartIcon">@drawable/ic_action_restart_dark</item>
		<item name="runIcon">@drawable/ic_action_run_dark</item>
		<item name="helpIcon">@drawable/ic_action_help_dark</item>

		<item name="aux_comp_back">@drawable/aux_comp_back_black</item>

		<item name="wireColor">@color/black</item>
		<item name="readColor">@color/readLight</item>
		<item name="writeColor">@color/writeLight</item>
		<item name="rwColor">@color/rwLight</item>
		<item name="instColor">@color/instLight</item>
		<item name="idColor">@color/idLight</item>
		<item name="ifColor">@color/ifLight</item>
		<item name="exColor">@color/exLight</item>
		<item name="memColor">@color/memLight</item>
		<item name="wbColor">@color/wbLight</item>
	</style>

</resources>